{
  "name": "studyspot-mobile",
  "version": "1.0.0",
  "description": "StudySpot Mobile Application - React Native with TypeScript",
  "main": "index.js",
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "build:android": "cd android && ./gradlew assembleRelease",
    "build:ios": "cd ios && xcodebuild -workspace StudySpot.xcworkspace -scheme StudySpot -configuration Release archive",
    "clean": "react-native clean",
    "reset-cache": "react-native start --reset-cache",
    "detox:build": "detox build",
    "detox:test": "detox test"
  },
  "dependencies": {
