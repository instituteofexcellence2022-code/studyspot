# StudySpot Mobile App - Production Environment Configuration

# API Configuration
API_BASE_URL=https://studyspot-api.onrender.com/api/v1
API_TIMEOUT=30000

# Environment
NODE_ENV=production
DEBUG_MODE=false
LOG_LEVEL=error

# Feature Flags
ENABLE_OFFLINE_MODE=true
ENABLE_ANALYTICS=true
ENABLE_CRASH_REPORTING=true
ENABLE_PUSH_NOTIFICATIONS=true

# App Configuration
APP_VERSION=1.0.0
BUILD_NUMBER=1
MINIMUM_APP_VERSION=1.0.0

# Payment Gateway (Razorpay - Production)
RAZORPAY_KEY_ID=rzp_live_PLACEHOLDER
RAZORPAY_KEY_SECRET=PLACEHOLDER_SECRET

# Firebase Configuration (Production)
FIREBASE_API_KEY=AIzaSyPLACEHOLDER
FIREBASE_AUTH_DOMAIN=studyspot-prod.firebaseapp.com
FIREBASE_PROJECT_ID=studyspot-prod
FIREBASE_STORAGE_BUCKET=studyspot-prod.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_APP_ID=1:123456789:android:PLACEHOLDER

# Google Maps
GOOGLE_MAPS_API_KEY=AIzaSyPLACEHOLDER_MAPS

# Social Authentication
GOOGLE_CLIENT_ID=PLACEHOLDER.apps.googleusercontent.com
FACEBOOK_APP_ID=PLACEHOLDER

# Push Notifications
ONESIGNAL_APP_ID=PLACEHOLDER-ONESIGNAL
ONESIGNAL_REST_API_KEY=PLACEHOLDER-REST-API

# Analytics
MIXPANEL_TOKEN=PLACEHOLDER_MIXPANEL
AMPLITUDE_API_KEY=PLACEHOLDER_AMPLITUDE

# NOTE: Replace all PLACEHOLDER values with actual production credentials
# Copy this file to .env for production builds
