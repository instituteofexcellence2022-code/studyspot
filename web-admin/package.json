{
  "name": "studyspot-web-admin",
  "version": "0.1.0",
  "description": "⚙️ STUDYSPOT Platform Administrator Portal",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^5.16.7",
    "@mui/lab": "^5.0.0-alpha.173",
    "@mui/material": "^5.16.7",
    "@mui/x-data-grid": "^7.29.9",
    "@mui/x-date-pickers": "^7.29.4",
    "@reduxjs/toolkit": "^2.9.1",
    "@sentry/react": "^10.22.0",
    "@sentry/tracing": "^7.120.4",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/react-router-dom": "^5.3.3",
    "axios": "^1.12.2",
    "date-fns": "^4.1.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.65.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.30.1",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "recharts": "^3.3.0",
    "redux-persist": "^6.0.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "cross-env PORT=3002 NODE_OPTIONS=--max_old_space_size=1536 ESLINT_NO_DEV_ERRORS=true craco start",
    "build": "cross-env TSC_COMPILE_ON_ERROR=true BABEL_ENV=production NODE_OPTIONS=--max_old_space_size=2048 GENERATE_SOURCEMAP=false craco build",
    "build:dev": "cross-env NODE_OPTIONS=--max_old_space_size=1536 craco build",
    "build:prod": "cross-env TSC_COMPILE_ON_ERROR=true BABEL_ENV=production NODE_OPTIONS=--max_old_space_size=2048 GENERATE_SOURCEMAP=false craco build",
    "analyze": "cross-env NODE_OPTIONS=--max_old_space_size=2048 GENERATE_SOURCEMAP=false craco build --analyze",
    "analyze:bundle": "node scripts/analyze-bundle.js",
    "analyze:deps": "node analyze-dependencies.js",
    "analyze:routes": "node identify-unused-routes.js",
    "perf:test": "npm run build && npm run analyze:bundle",
    "test": "craco test --setupFilesAfterEnv=src/__tests__/setup.ts",
    "test:watch": "craco test --watch",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:integration": "jest --config=jest.integration.config.js",
    "test:e2e": "playwright test",
    "test:a11y": "jest --config=jest.a11y.config.js",
    "lint": "eslint src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "security:scan": "snyk test",
    "accessibility:audit": "node scripts/accessibility-audit.js",
    "performance:audit": "node scripts/performance-audit.js",
    "bundle:analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "precommit": "npm run lint && npm run type-check && npm run test:coverage",
    "prepush": "npm run precommit && npm run build",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@craco/craco": "^7.1.0",
    "@types/axios": "^0.9.36",
    "@types/node": "^24.9.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "cross-env": "^10.1.0",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-react-hooks": "^7.0.0",
    "eslint-plugin-testing-library": "^6.2.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.40.0",
    "prettier": "^3.1.0",
    "snyk": "^1.1248.0",
    "typescript": "^5.9.3",
    "webpack-bundle-analyzer": "^4.10.1"
  }
}
}

